            "0x87084ec881d5A15C918057F326790dB177D218F2",
            "0x9F9c171aFdE4cc6bBF6d38aE4012c83633653B85",
            "0x369156da04B6F313b532F7aE08E661e402B1C2F2",
            "0xBA627f3d081cc97ac0eDc40591eda7053AC63532",
            "0x454cBC099079DC38b145E37e982e524AF3279c44",
            "0x950b9476a4de757BB134483029AC4Ec17E739e3A"




        ];

			const options = {
				method: 'GET',
				headers: {
					accept: 'application/json'
				}
			};

			/******************************************* Mapping for endpoint *****************************************/
			Promise.all(communityAddresses.map(address => {
				const url = `https://eth-mainnet.g.alchemy.com/nft/v2/${apiKey}/isHolderOfCollection?wallet=${walletAddress}&contractAddress=${address}`;
				return fetchWithRetry(url, options).then(response => response.json());
			}))
			.then(responses => {
				/******************************************* Store Response(boolean) *****************************************/
				const isHolder = responses.some(response => response.isHolderOfCollection);
				if (isHolder) {
					/******************************************* Routing to Dashboard - Button text update II *****************************************/
					connectButton.innerText = 'Access Granted';
					console.log("User is a holder of at least one collection");
					// Create a new XMLHttpRequest object
					var xhr = new XMLHttpRequest();
					// Prepare the AJAX request
					xhr.open('POST', Wo_Ajax_Requests_File() + '?f=register_wallet', true);
					// Define success and failure handlers
					xhr.onload = function() {
						if (xhr.status >= 200 && xhr.status < 400) {
							var response = xhr.responseText;
							if (isJson(response) === true) {
								const responseJSON = JSON.parse(response);
								if (responseJSON.errors) {
									alert(responseJSON.errors[0]);
								} else {
									window.location.href = responseJSON.location;
								}
							} else {
								window.location.href = "http:\/\/localhost\/BlokZoo\/";
							}
						} else {
							console.log('Error:', xhr.status);
						}
					};
					// Data to be sent
					var data = {
						name: 'John Doe',
						email: 'johndoe@example.com'
					};
					var jsonData = JSON.stringify(data);
					var formdata = new FormData();
					formdata.append("wallet", accounts[0]);
					formdata.append("username", accounts[0]);
					formdata.append("email", accounts[0] + '@metamask.com');
					formdata.append("password", 'User@MetaMask');
					formdata.append("confirm_password", 'User@MetaMask');
					formdata.append("gender", 'Male');
					formdata.append("accept_terms", accounts[0]);
					xhr.send(formdata);
				} else {
					/******************************************* No Routing - Button text update III *****************************************/
					connectButton.innerText = 'Not Whitelisted';
					console.log("User is not a holder of any collection");
				}
			})
			.catch(err => console.error(err));
		} catch (error) {
			console.error("Error connecting to wallet:", error);
		}
	}

	function isJson($string = "") {
		try {
			JSON.parse($string);
			return true;
		} catch (error) {
			return false;
		}
	}
</script>

<script>
$(document).on('click', '#night_mode_toggle', function(event) {
  mode = $(this).attr('data-mode');
  if (mode == 'night') {
      $('head').append('<link rel="stylesheet" href="<?php echo $wo['config']['theme_url'];?>/stylesheet/dark.css<?php echo $wo['update_cache']; ?>" id="night-mode-css">');
      $('#night_mode_toggle').attr('data-mode', 'day');
      $(this).addClass('night');
	  $(this).removeClass('day');
	  replaceLogos('night');
  } else {
  		replaceLogos('day');
      $('#night-mode-css').remove();
      $('#night_mode_toggle').attr('data-mode', 'night');
      $(this).removeClass('night');
	  $(this).addClass('day');
  }
  $.post(Wo_Ajax_Requests_File() + '?mode=' + mode);
});



   async function getAccount() {
      const accounts = await window.ethereum.request({
            method: 'eth_requestAccounts'
         })
         .catch((err) => {
            if (err.code === 4001) {
               // EIP-1193 userRejectedRequest error
               // If this happens, the user rejected the connection request.
               console.log('Please connect to MetaMask.');
            } else {
               console.error(err);
            }
         });
      const account = accounts[0];
      showAccount.innerHTML = account;
   }

   function isJson($string = "") {
      try {
         JSON.parse($string);
         return true;
      } catch (error) {
         return false;
      }
   }
</script>
