      const projectList = document.getElementById('project-list');
      const filterInput = document.getElementById('filter');

      // Function to filter projects based on user input
      function filterProjects() {
        const filterValue = filterInput.value.toLowerCase();

        // Clear the project list
        projectList.innerHTML = '';

        // Filter the projects based on the user input
        const filteredProjects = allowlist.filter(project => {
          const projectName = project.name.toLowerCase();
          return projectName.includes(filterValue);
        });

        // Display the filtered projects
        filteredProjects.forEach(project => {
          const projectItem = document.createElement('div');
          projectItem.classList.add('project-item');

          const projectName = document.createElement('p');
          projectName.classList.add('project-name');
          projectName.textContent = project.name;

          const projectNetwork = document.createElement('p');
          projectNetwork.classList.add('project-network');
          projectNetwork.textContent = `Network: ${project.network}`;

          projectItem.appendChild(projectName);
          projectItem.appendChild(projectNetwork);
          projectList.appendChild(projectItem);
        });
      }

      // Event listener for the filter input
      filterInput.addEventListener('input', filterProjects);
    });

    // Open Google Form
    function openGoogleForm() {
      // Replace with your Google Form URL
      const googleFormUrl = 'https://docs.google.com/forms/d/e/1FAIpQLSfKkhhQ_Ph4tyVpzurly8f4SUkjlRJzmfgdQi1UwwgsJJkjcw/viewform?usp=sf_link';

      window.open(googleFormUrl, '_blank');
    }
  </script>
</body>
</html>




			
            

