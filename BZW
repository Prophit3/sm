<script>
    // New fetch with retry function
	async function fetchWithRetry(url, options, retryCount = 3) {
		const response = await fetch(url, options);
		if (response.status === 429 && retryCount > 0) {
			await new Promise(resolve => setTimeout(resolve, 1000));
			return fetchWithRetry(url, options, retryCount - 1);
		}
		return response;
	}
	/******************************************* OnClick function for NFT Token Gating *****************************************/
	async function connectWallet() {

		/******************************************* ether js - rpc using Infura (Sepolia network) *****************************************/
		const {
			ethers
		} = window;
		const provider = new ethers.providers.JsonRpcProvider("https://mainnet.infura.io/v3/42cb9984b84344e1a2be8a1e0a37b233");

		try {

			/******************************************* Button text update I *****************************************/
			const connectButton = document.getElementById('connect-wallet');
			connectButton.innerText = 'Checking For Access...';
			event.preventDefault();

			/******************************************* Connect to Wallet *****************************************/
			const accounts = await ethereum.request({
				method: 'eth_requestAccounts'
			});
			const signer = provider.getSigner(accounts[0]);
			console.log("Connected to wallet:", accounts[0]);

			/******************************************* Alchemy - api Key - walletAddress - whitelistedCommunity *****************************************/

